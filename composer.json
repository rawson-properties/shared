{
    "name": "rawson-properties/shared",
    "description": "Shared resources for Rawson Properties projects",
    "authors": [],
    "keywords": [],
    "license": "na",
    "require": {
        "fzaninotto/faker": "~1.4",
        "php": "^7.2.0",
        "laravel/framework": "^6.0.0",
        "mixpanel/mixpanel-php": "^2.6",
        "ryanwinchester/hubspot-php": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0"
    },
    "suggest": {
        "laravel/socialite": "Required if you're going to make use of OAuth/SSO."
    },
    "autoload": {
        "psr-4": {
            "Rawson\\Shared\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rawson\\Shared\\Providers\\RawsonSharedServiceProvider",
                "Rawson\\Shared\\Providers\\PingServiceProvider"
            ]
        }
    }
}
